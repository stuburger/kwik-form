{
  "name": "yafl",
  "version": "1.0.0-rc.5",
  "description": "Yet another form library for React",
  "main": "dist/index.js",
  "module": "dist/yafl.esm.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stuburger/yafl"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:watch": "npm run test -- --watchAll",
    "start": "cross-env NODE_ENV=development tsdx watch --verbose",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdx build --format=cjs,esm,umd",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "gen-docs": "all-contributors generate && doctoc README.md",
    "size": "size-limit",
    "lint": "tsdx lint"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "tsdx lint",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "object-path-immutable": "^4.1.0",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^3.0.1",
    "tiny-warning": "^1.0.3",
    "tslib": "^1.11.1",
    "tslint": "^6.1.0"
  },
  "keywords": [
    "yafl",
    "react",
    "form",
    "context",
    "validation"
  ],
  "peerDependencies": {
    "react": ">=16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@size-limit/preset-big-lib": "^4.4.0",
    "@testing-library/react": "^10.0.1",
    "@types/invariant": "^2.2.31",
    "@types/jest": "^25.1.4",
    "@types/node": "13.9.2",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "cross-env": "^7.0.2",
    "doctoc": "^1.3.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "size-limit": "^4.4.0",
    "ts-jest": "^25.2.1",
    "tsdx": "^0.13.0",
    "typescript": "^3.8.3",
    "uglify-es": "^3.3.9",
    "why-did-you-update": "^1.0.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "10kb"
    },
    {
      "path": "./dist/yafl.esm.js",
      "limit": "10kb"
    },
    {
      "path": "./dist/yafl.umd.production.min.js",
      "limit": "10kb"
    }
  ]
}
