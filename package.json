{
  "name": "yafl",
  "version": "1.0.0-ctx-4",
  "description": "Yet another form library for React",
  "main": "lib/yafl.js",
  "module": "lib/yafl.esm.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stuburger/yafl"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "size": "size-limit",
    "toc": "doctoc README.md",
    "prebuild": "rimraf lib",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.base.json  && rollup -c && rimraf compiled && cp ./index.js ./lib/index.js",
    "watch": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "jest:updatesnapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {
    "hoist-non-react-statics": "^3.1.0",
    "invariant": "^2.2.4",
    "object-path-immutable": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^2.0.2",
    "tslib": "^1.9.3"
  },
  "keywords": [
    "yafl",
    "react",
    "form",
    "context",
    "validation"
  ],
  "peerDependencies": {
    "react": ">=16.3.2"
  },
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.9",
    "@types/node": "10.12.2",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "cross-env": "^5.2.0",
    "doctoc": "^1.3.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-test-renderer": "^16.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.0",
    "size-limit": "^0.21.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6",
    "uglify-es": "^3.3.9",
    "why-did-you-update": "^0.2.0"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "size-limit": [
    {
      "path": "./lib/index.js",
      "limit": "16 kB"
    },
    {
      "path": "./lib/yafl.esm.js",
      "limit": "16 kB"
    },
    {
      "path": "./lib/yafl.umd.production.js",
      "limit": "16 kB"
    }
  ]
}
