{
  "name": "yafl",
  "version": "1.0.0",
  "description": "A library for creating forms in react using react's new context api",
  "main": "lib/yafl.js",
  "module": "lib/yafl.es6.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.base.json  && rollup -c && rimraf compiled && cp ./index.js ./lib/index.js",
    "watch": "tsc --watch",
    "test": "jest",
    "jest:updatesnapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {    
    "invariant": "^2.2.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.omit": "^4.5.0",
    "lodash.set": "^4.3.2",
    "object-path-immutable": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^2.0.1"
  },
  "keywords": [
    "react",
    "form",
    "context"
  ],
  "peerDependencies": {
    "react": ">=16.3.2"
  },
  "devDependencies": {
		"@types/invariant": "^2.2.29",
		"@types/jest": "^23.1.5",
    "@types/lodash.clonedeep": "^4.5.3",
    "@types/lodash.defaultsdeep": "^4.6.3",
		"@types/lodash.get": "^4.4.3",
		"@types/lodash.omit": "^4.5.3",
    "@types/lodash.set": "^4.3.3",
    "@types/node": "10.5.2",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-test-renderer": "^16.0.1",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2",
    "uglify-es": "^3.3.9",
    "why-did-you-update": "^0.1.1"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
