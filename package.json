{
  "name": "yafl",
  "version": "1.0.0-rc.1",
  "description": "Yet another form library for React",
  "main": "lib/yafl.js",
  "module": "lib/yafl.esm.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stuburger/yafl"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "size": "size-limit",
    "toc": "doctoc README.md",
    "prebuild": "rimraf lib",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.base.json  && rollup -c && rimraf compiled && cp ./index.js ./lib/index.js",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "jest:updatesnapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "object-path-immutable": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^2.0.2",
    "tiny-warning": "^1.0.2",
    "tslib": "^1.10.0"
  },
  "keywords": [
    "yafl",
    "react",
    "form",
    "context",
    "validation"
  ],
  "peerDependencies": {
    "react": ">=16.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^24.0.15",
    "@types/node": "12.0.8",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "cross-env": "^5.2.0",
    "doctoc": "^1.3.1",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^8.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.9.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.0",
    "size-limit": "^1.3.7",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "uglify-es": "^3.3.9",
    "why-did-you-update": "^1.0.6"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}"
    ],
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "size-limit": [
    {
      "path": "./lib/index.js",
      "limit": "16 kB"
    },
    {
      "path": "./lib/yafl.esm.js",
      "limit": "16 kB"
    },
    {
      "path": "./lib/yafl.umd.production.js",
      "limit": "16 kB"
    }
  ]
}
