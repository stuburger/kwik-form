{
  "name": "yafl",
  "version": "1.0.0-rc.6",
  "description": "Yet another form library for React",
  "main": "dist/index.js",
  "module": "dist/yafl.esm.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/stuburger/yafl"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "size": "size-limit",
    "toc": "doctoc README.md",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.json && rollup -c && rimraf compiled && cp ./index.js ./dist/index.js",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "jest:updatesnapshot": "jest --updateSnapshot",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "tsdx lint",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "object-path-immutable": "^4.1.0",
    "prop-types": "^15.6.2",
    "react-fast-compare": "^3.0.1",
    "tiny-warning": "^1.0.3",
    "tslib": "^1.11.1",
    "tslint": "^6.1.0"
  },
  "keywords": [
    "yafl",
    "react",
    "form",
    "context",
    "validation"
  ],
  "peerDependencies": {
    "react": ">=16.13.0"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.{ts,tsx}"
    ],
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "10kb"
    },
    {
      "path": "./dist/yafl.esm.js",
      "limit": "10kb"
    },
    {
      "path": "./dist/yafl.umd.production.min.js",
      "limit": "10kb"
    }
  ]
}
